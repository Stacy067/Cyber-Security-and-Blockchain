vagrant up

vagrant ssh

vim test.c

#include <stdio.h>
void add(int a, int b) {
        int c = a + b;
        printf("%d\n", c);
}

int main(){
        add(5, 10);
        return 0;
}


gcc -o test test.c
ls
./test


